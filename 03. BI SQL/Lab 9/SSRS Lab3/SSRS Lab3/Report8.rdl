    <DataSet Name="MyGrade">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@cid">
            <Value>=Parameters!cid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Course.Crs_Id, Course.Crs_Name, Student.St_Id, Student.St_Fname, Stud_Course.Grade
FROM Stud_Course 
INNER JOIN Student ON Stud_Course.St_Id = Student.St_Id
INNER JOIN Course ON Stud_Course.Crs_Id = Course.Crs_Id
INNER JOIN Course AS Course_1 ON Stud_Course.Crs_Id = Course_1.Crs_Id
WHERE Stud_Course.Crs_Id = @cid</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
